            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="btnAddMember" alignment="CENTER" contentDisplay="CENTER" layoutX="182.0" layoutY="119.0" mnemonicParsing="false" onAction="#addMemberClicked" prefHeight="30.0" prefWidth="238.0" text="Add Member">
         <font>
            <Font size="14.0" />
         </font></Button>
      <Button fx:id="btnEditMember" layoutX="182.0" layoutY="176.0" mnemonicParsing="false" onAction="#editMemberClicked" prefHeight="30.0" prefWidth="238.0" text="Edit Member">
