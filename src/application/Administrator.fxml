            <Font size="14.0" />
         </font></Button>
      <Button fx:id="btnAddBook" layoutX="182.0" layoutY="234.0" mnemonicParsing="false" onAction="#addBookClicked" prefHeight="30.0" prefWidth="238.0" text="Add New Book">
         <font>
            <Font size="14.0" />
         </font></Button>
      <Button fx:id="btnAddBookCopy" layoutX="182.0" layoutY="289.0" mnemonicParsing="false" onAction="#addBookCopyClicked" prefHeight="30.0" prefWidth="238.0" text="Add Book Copy">
